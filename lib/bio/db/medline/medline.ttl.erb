<%= uri %>	<%= predicate('sb') %>	"<%=c pubmed['SB'] %>" .
<%= uri %>	<%= predicate('pt') %>	"<%=c pubmed['PT'] %>" .
<% pubmed['FAU'].chomp.split("\n").each do |fau| %>

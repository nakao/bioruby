<%= uri %>	<http://purl.org/dc/terms/creator>	"<%=c fau %>" .
<% end %>
<% pubmed['AU'].chomp.split("\n").each do |au| %>
<%= uri %>	<%= predicate('au') %>	"<%=c au %>" .

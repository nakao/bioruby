<%= uri %>	<%= predicate('pg') %>	"<%=c pubmed['PG'] %>" .
<%= uri %>	<http://prismstandard.org/namespaces/1.2/basic/startingPage>	"<%=c pubmed['PG'].split("-").first %>" .
